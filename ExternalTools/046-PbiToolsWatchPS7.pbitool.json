{
    "version": "0.0.1",
    "name": "pbi-tools: Watch",
    "author": "@JamesDBartlett3@techhub.social",
    "description": "Launches pbi-tools in a PowerShell window, and automatically begins watching the current file for changes to extract.",
    "installation": "Save a copy of this file in 'C:\\Program Files (x86)\\Common Files\\Microsoft Shared\\Power BI Desktop\\External Tools' and re-launch Power BI Desktop.",
    "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
    "arguments": "-NoLogo -ExecutionPolicy Bypass -NoExit -Command pbi-tools extract -pid $(((Get-Process -Id (Get-NetTCPConnection -LocalPort '%server%'.Split(':')[1]).OwningProcess).Parent).Id) -watch",
    "iconData": "data:image/png;base64,"
}
